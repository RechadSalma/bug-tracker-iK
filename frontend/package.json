{
  "name": "frontend-bug-tracker",
  "version": "1.0.0",
  "description": "frontend for the bug tracker",
  "private": true,
  "scripts": {
    "ikupdate": "npm i && npm update && npm audit fix --force && npm outdated",
    "build": "eslint . && rm -rfv ./dist && mkdir dist && cp -rv ./src/public/manifest/ ./dist/ && webpack --config webpack.production.config.js",
    "prod": "npm run build && node src/server.js",
    "dev": "eslint . && rm -rfv ./dist && mkdir dist && cp -rv ./src/public/manifest ./dist && webpack serve --config webpack.dev.config.js --hot",
    "server": "node src/server.js",
    "lint": "eslint .",
    "ikclean": "rm -fr node_modules/ package-lock.json dist/ && npm cache clean -vf && npm run ikupdate && npm run dev",
    "ikcleanci": "rm -fr node_modules/ dist/ && npm cache clean -vf && npm ci && npm run dev",
    "backend": "cd ../backend && npm run server"
  },
  "keywords": [],
  "author": "Rechad Salma",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@tsconfig/node16": "^1.0.2",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-helmet": "^6.1.5",
    "@types/uuid": "^8.3.4",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.17.0",
    "eslint-plugin-react": "^7.30.0",
    "handlebars-loader": "^1.7.2",
    "html-webpack-plugin": "^5.4.0",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "postcss-preset-env": "^7.7.1",
    "react-refresh": "^0.13.0",
    "sass": "^1.52.3",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.9.2",
    "workbox-webpack-plugin": "^6.5.3"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.10.1",
    "@reduxjs/toolkit": "^1.8.2",
    "axios": "^0.27.2",
    "bootstrap": "^5.1.3",
    "dotenv": "^16.0.1",
    "formik": "^2.2.9",
    "handlebars": "^4.7.7",
    "react": "^18.1.0",
    "react-bootstrap": "^2.4.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  }
}
